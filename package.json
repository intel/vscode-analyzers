{
	"name": "intel-performance-tools-launcher",
	"displayName": "Intel® Performance Tools Launcher",
	"description": "This extension enables launching Intel® Performance Tools, Intel VTune™ and Intel Advisor, and passing project specific parameters to the tool",
	"version": "0.0.2",
	"publisher": "Intel",
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:workbench.action.tasks.runTask",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "perfTools.launchEntry",
				"title": "Launch",
				"icon": {
					"light": "resources/rocket-launch.svg",
					"dark": "resources/rocket-launch.svg"
				}
			},
			{
				"command": "perfTools.launchAdvisor",
				"title": "Launch Advisor",
				"icon": {
					"light": "resources/rocket-launch.svg",
					"dark": "resources/rocket-launch.svg"
				}
			},
			{
				"command": "perfTools.launchVTune",
				"title": "Launch VTune",
				"icon": {
					"light": "resources/rocket-launch.svg",
					"dark": "resources/rocket-launch.svg"
				}
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "perfTools.launchAdvisor"
				},
				{
					"command": "perfTools.launchVTune"
				}
			]
		},
		"configuration": [
			{
				"title": "Intel Performance Tools",
				"properties": {
					"intel.advisor.install-root": {
						"type": "string",
						"description": "Root install location for Intel Advisor",
						"scope": "window"
					},
					"intel.vtune.install-root": {
						"type": "string",
						"description": "Root install location for Intel VTune",
						"scope": "window"
					},
					"application.executable-path": {
						"type": "string",
						"description": "Path of the executable to analyze",
						"scope": "resource"
					},
					"advisor.project-folder": {
						"type": "string",
						"default": "./advisor",
						"description": "Path of the Intel Advisor Project Folder",
						"scope":"resource"
					},
					"vtune.project-folder": {
						"type": "string",
						"default": "./vtune",
						"description": "Path of the Intel VTune Project Folder",
						"scope":"resource"
					}
				}
			}
		],
		"taskDefinitions": [
			{
				"type": "toolProvider",
				"properties": {
					"test1": {
						"type": "string",
						"description": "Testing"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"package": "vsce package --yarn --baseContentUrl https://gitlab.devtools.intel.com/sip-oneapi/vscode-launcher-plugin/raw/master/"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^10.12.21",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.36.0",
		"typescript": "^3.5.1",
		"vsce": "^1.71.0",
		"tslint": "^5.12.1"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2",
		"ftp": "^0.3.10",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"promisify-child-process": "^3.1.1"
	}
}